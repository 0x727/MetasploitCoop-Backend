{
    "python.autoComplete.extraPaths": [
        "./homados",
        "./homados/apps"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/venv/**": true,
        "**/*.pyc": true,
        "**/node_modules/**": true
    },
    "search.exclude": {
        "**/*.pyc": true,
        "**/venv": true
    },
    "python.linting.pylintArgs": [
        "--max-line-length", "120",
        "--load-plugins", "pylint_django",
        "--init-hook", "import sys; sys.path.extend(['${workspaceFolder}/homados', './homados/apps']);"
    ],
    "python.analysis.extraPaths": [
        "./homados",
        "./homados/apps"
    ],
    "python.pythonPath": "venv/bin/python"
}